<?xml version='1.0' encoding='utf-8'?>
<widget id="org.js.christo.urinz" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>Urinz</name>
    <description>
        Urine validations
    </description>
    <author email="andrechristoga@gmail.com" href="http://christo.js.org">
        Andre Christoga
    </author>
    <icon src="www/img/logo.png" />
    <content src="www/index.html" />
    <access origin="*" />
</widget>
